{
  "manifest_version": 2,
  "name": "Streamline",
  "version": "0.0.1",

  "icons": {  "16": "icon16.png",
              "48": "icon48.png",
              "128": "icon128.png"
  },

  "browser_action":{
    "default_icon": "icon16.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "permissions": [
    "tabs",
    "notifications"
  ]
}
